{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["src/"],
        "includePattern": ".+\\.(js(doc|x)?|mjs|vue)$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/jsdoc-vuejs"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}